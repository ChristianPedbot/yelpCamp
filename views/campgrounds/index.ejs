<!-- index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Campgrounds</title>
</head>
<body>
    <h1>Lista de Campgrounds</h1>
    <ul>
        <% campgrounds.forEach(campground => { %>
            <li><%= campground.title %> - <%= campground.price %></li>
        <% }) %>
    </ul>
</body>
</html>
